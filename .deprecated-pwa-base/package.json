  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "prettier": "prettier --write ./src",
    "translations:extract": "yarn run translations:extract:internal  \"src/**/*.ts*\" --ignore=\"**/*.d.ts\" --out-file \"lang/extracted.json\" --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "translations:extract:internal": "formatjs extract",
    "compile": "formatjs compile"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@formatjs/cli": "^6.1.1",
    "@storybook/addon-essentials": "7.0.5",
    "@storybook/addon-interactions": "7.0.5",
    "@storybook/addon-links": "7.0.5",
    "@storybook/blocks": "7.0.5",
    "@storybook/preset-create-react-app": "7.0.5",
    "@storybook/react": "7.0.5",
    "@storybook/react-webpack5": "7.0.5",
    "@storybook/testing-library": "0.0.14-next.2",
    "@types/ajv": "^1.0.0",
    "babel-plugin-named-exports-order": "0.0.2",
    "prettier": "2.8.7",
    "prop-types": "15.8.1",
    "storybook": "7.0.5",
    "webpack": "5.79.0"
  }
}
